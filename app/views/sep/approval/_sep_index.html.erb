<style type="text/css">
  table.dataTable.select tbody tr,
  table.dataTable thead th:first-child {
     cursor: pointer;
  }

  td.add-control, td.history-control {
    cursor: pointer;
  }
 
  div.slider {
    display: none;
  }
 
  table.dataTable tbody td.no-padding {
    padding: 0;
  }
  
  table.dataTable .hiddenRow {
    padding: 0 !important;
  }
}
</style>